{
  "name": "propeteer/propeteer",
  "description": "Propeteer is a property management system",
  "license": "AGPL-3.0-or-later",
  "version": "0.1.0",
  "autoload": {
    "psr-4": {
      "src\\Abstracts\\": "src/Abstracts/",
      "src\\Controllers\\Api\\": "src/Controllers/Api/",
      "src\\Controllers\\Web\\": "src/Controllers/Web/",
      "src\\Core\\": "src/Core/",
      "src\\Enums\\": "src/Enums/",
      "src\\Exceptions\\": "src/Exceptions/",
      "src\\Factories\\": "src/Factories/",
      "src\\Forms\\": "src/Forms/",
      "src\\Interfaces\\": "src/Interfaces/",
      "src\\Models\\": "src/Models/",
      "src\\Repositories\\": "src/Repositories/",
      "src\\Services\\": "src/Services/",
      "src\\Validation\\": "src/Validation/",
      "src\\Views\\": "src/Views/"
    },
    "files": [
      "src/Core/config.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/"
    }
  },
  "require": {
    "ext-curl": "*",
    "ext-pdo": "*",
    "php": ">=8.4",
    "twig/twig": "v3.21.1",
    "phpmailer/phpmailer": "^6.10"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "squizlabs/php_codesniffer": ">=4.0.0RC1"
  },
  "scripts": {
    "phpcs": "phpcs --standard=phpcs.xml"
  }
}
